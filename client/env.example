# Vite environment variables for GRIMPE client
# Copy this file to .env or .env.local and adjust values as needed

# API URL - defaults to relative path with proxy
VITE_API_URL=/api

# Alternative: direct API URL (if not using proxy)
# VITE_API_URL=http://localhost:5000/api

# App information
VITE_APP_NAME=GRIMPE
VITE_APP_VERSION=1.0.0

# Development settings
VITE_DEV_MODE=true 